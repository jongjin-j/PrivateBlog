<!DOCTYPE html>
<html lang="en">

  <%- include('../partials/head.ejs') %>
  
  <body>

    <%- include('../partials/nav.ejs') %>

    <div>
      <h2>All Blogs</h2>
    </div>
    <% if(blogs.length > 0) { %>
      <% blogs.forEach(blog => { %>

        <h3><%= blog.title %></h3>
        <p><%= blog.location %></p>

      <% }) %>
    <% } %>
  </body>
</html>
